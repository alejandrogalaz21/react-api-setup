    "type": "git",
    "url": "git+https://github.com/alejandrogalaz21/node-babel-server-.git"
  },
  "keywords": [
    "node",
    "node.js",
    "babel",
    "node-babel",
    "javascript",
    "js"
  ],
  "author": "alejandro galaz",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
